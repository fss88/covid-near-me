
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle">
        <label>Picture</label>
        <input class="form-control" type="url" name="picture">
        <label>Content</label>
        <textarea class="form-control" name="postBody" rows="20" cols="30"></textarea>
        <label>Author</label>
        <textarea class="form-control" name="author" rows="1" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>

<%- include("partials/footer"); -%>
